JAVAC           = @JAVAC@
RM           = @RM@

CLASSPATH=@CLASSPATH@:"$HDFVIEW_HOME"/lib/junit4.jar

.SUFFIXES: .java .class

.java.class:
	$(JAVAC) -classpath $(CLASSPATH) $<

all:    TestH5.class \
        TestH5E.class \
        TestH5Edefault.class \
        TestH5Eregister.class \
        TestH5Fparams.class \
        TestH5Fbasic.class \
        TestH5F.class \
        TestH5Gbasic.class \
        TestH5G.class \
        TestH5Giterate.class \
        TestH5Sbasic.class \
        TestH5S.class \
        TestH5Tparams.class \
        TestH5Tbasic.class \
        TestH5T.class \
        TestH5Dparams.class \
        TestH5D.class \
        TestH5Lparams.class \
        TestH5Lbasic.class \
        TestH5Lcreate.class \
        TestAll.class 


check: all
	chmod u+x junit.sh; # ./junit.sh

clean:
	$(RM) *.class
	$(RM) -f *.hdf
	$(RM) -f *.h5

