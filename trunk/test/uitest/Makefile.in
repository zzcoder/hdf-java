JAVAC           = @JAVAC@
RM           = @RM@

CLASSPATH=@CLASSPATH@:../../lib/junit.jar:../../lib/fest-swing-1.2.jar:../../lib/jcip-annotations-1.0.jar

.SUFFIXES: .java .class

.java.class:
	$(JAVAC) -classpath $(CLASSPATH) $<

all: \
	TestHDFView.class \
	AllTests.class 


check: all
	chmod u+x junit.sh; ./junit.sh

clean:
	$(RM) *.class
	$(RM) -f *.h5

