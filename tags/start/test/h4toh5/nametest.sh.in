#!/bin/sh

JH45INSTALLDIR=@JH45INST@

LIBDIR=$JH45INSTALLDIR"/lib"

CLASSPATH=".:"$LIBDIR"/jh4toh5.jar:"$LIBDIR"/jhdf.jar:"$LIBDIR"/jhdf5.jar"

LD_LIBRARY_PATH=$LIBDIR"/@JAVATARG@"

export CLASSPATH
export LD_LIBRARY_PATH

@JAVA@ -Djava.library.path=$LD_LIBRARY_PATH -Dncsa.hdf.hdf5lib.H5.hdf5lib=$LIBDIR"/@JAVATARG@/libjh4toh5.so" -Dncsa.hdf.h4toh5lib.h4toh5.h45lib=$LIBDIR"/@JAVATARG@/libjh4toh5.so" -Dncsa.hdf.hdflib.HDFLibrary.hdflib=$LIBDIR"/@JAVATARG@/libjh4toh5.so" nametest
